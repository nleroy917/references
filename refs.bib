@article{Shi2020,
  title = {{{HiChIP-Peaks}}: a {{HiChIP}} peak calling algorithm},
  shorttitle = {{{HiChIP-Peaks}}},
  author = {Shi, Chenfu and Rattray, Magnus and Orozco, Gisela},
  year = {2020},
  month = jun,
  journal = {Bioinformatics},
  volume = {36},
  number = {12},
  pages = {3625--3631},
  issn = {1367-4803},
  doi = {10.1093/bioinformatics/btaa202},
  urldate = {2023-06-27},
  abstract = {HiChIP is a powerful tool to interrogate 3D chromatin organization. Current tools to analyse chromatin looping mechanisms using HiChIP data require the identification of loop anchors to work properly. However, current approaches to discover these anchors from HiChIP data are not satisfactory, having either a very high false discovery rate or strong dependence on sequencing depth. Moreover, these tools do not allow quantitative comparison of peaks across different samples, failing to fully exploit the information available from HiChIP datasets.We develop a new tool based on a representation of HiChIP data centred on the re-ligation sites to identify peaks from HiChIP datasets, which can subsequently be used in other tools for loop discovery. This increases the reliability of these tools and improves recall rate as sequencing depth is reduced. We also provide a method to count reads mapping to peaks across samples, which can be used for differential peak analysis using HiChIP data.HiChIP-Peaks is freely available at https://github.com/ChenfuShi/HiChIP\_peaks.Supplementary data are available at Bioinformatics online.}
}

@misc{Shuai2021,
  title = {Generative {{Language Modeling}} for {{Antibody Design}}},
