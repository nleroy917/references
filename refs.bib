  author = {DUMITRU, Adrian and PROCOP, Alexandru and ILIESIU, Andreea and TAMPA, Mircea and MITRACHE, Luminita and COSTACHE, Mariana and SAJIN, Maria and LAZAROIU, Anca and CIRSTOIU, Monica},
