@article{Sloan2016,
  title = {{{ENCODE}} data at the {{ENCODE}} portal},
  author = {Sloan, Cricket A. and Chan, Esther T. and Davidson, Jean M. and Malladi, Venkat S. and Strattan, J. Seth and Hitz, Benjamin C. and Gabdank, Idan and Narayanan, Aditi K. and Ho, Marcus and Lee, Brian T. and Rowe, Laurence D. and Dreszer, Timothy R. and Roe, Greg and Podduturi, Nikhil R. and Tanaka, Forrest and Hong, Eurie L. and Cherry, J. Michael},
