@article{Du2021,
  title = {Targeting {{AURKA}} in {{Cancer}}: molecular mechanisms and opportunities for {{Cancer}} therapy},
  shorttitle = {Targeting {{AURKA}} in {{Cancer}}},
  author = {Du, Ruijuan and Huang, Chuntian and Liu, Kangdong and Li, Xiang and Dong, Zigang},
